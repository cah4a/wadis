{
   "name": "wadis",
   "version": "1.0.0",
   "description": "Wadis is a WebAssembly (Wasm) build of the RedisÂ® in-memory datastore, purpose-built for automated testing. ðŸ§ª",
   "repository": "https://github.com/cah4a/wadis",
   "type": "module",
   "license": "Redis Source Available License v2 (RSALv2)",
   "author": "Sancha",
   "main": "dist/index.cjs",
   "module": "dist/index.js",
   "types": "dist/index.d.ts",
   "engines": {
      "node": ">=20.0.0",
      "bun": ">=1.1.0"
   },
   "files": [
      "dist",
      "src",
      "README.md",
      "LICENSE"
   ],
   "packageManager": "pnpm@10.19.0",
   "scripts": {
      "build": "rm -rf dist && tsup",
      "build:wasm": "./compile && pnpm run build",
